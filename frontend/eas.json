{
  "cli": {
    "version": ">= 7.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "node": "20.11.1"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "buildConfiguration": "Release",
        "credentialsSource": "local"
      },
      "android": {
        "buildType": "apk"
      },
      "node": "20.11.1"
    },
    "production": {
      "autoIncrement": true,
      "node": "20.11.1"
    },
    "testflight": {
      "extends": "production",
      "distribution": "store",
      "releaseChannel": "testflight",
      "ios": {
        "simulator": false,
        "credentialsSource": "remote"
      },
      "node": "20.11.1"
    }
  },
  "submit": {
    "production": {},
    "testflight": {
      "extends": "production",
      "ios": {
        "appleId": "REPLACE_WITH_YOUR_APPLE_ID_EMAIL",
        "ascAppId": "REPLACE_WITH_YOUR_APP_STORE_CONNECT_APP_ID",
        "appleTeamId": "REPLACE_WITH_YOUR_APPLE_DEVELOPER_TEAM_ID"
      }
    }
  }
}
